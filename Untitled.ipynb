   "source": [
    "#FACTORIAL OF A NUMBER\n",
    "\n",
    "n = int(input(\"Enter n : \"))\n",
    "fact = 1\n",
    "for i in range(1, n+1):\n",
    "    fact *= i\n",
    "    if (i < n):\n",
    "        print(f\" {i} x\", end=\"\")\n",
    "    else:\n",
    "        print(f\" {i} \", end=\"\")\n",
    "print(\"=\",fact)"
   ]
